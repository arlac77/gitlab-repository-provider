{
  "name": "gitlab-repository-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/gitlab-provider.mjs",
  "exports": {
    ".": "./src/gitlab-provider.mjs"
  },
  "description": "repository provider for gitlab hosted repos",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/gitlab-provider.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/gitlab-provider.mjs"
  },
  "dependencies": {
    "content-entry": "^2.6.0",
    "matching-iterator": "^1.1.0",
    "node-fetch": "3.0.0-beta.9",
    "one-time-execution-method": "^2.0.3",
    "repository-provider": "^25.0.7"
  },
  "devDependencies": {
    "ava": "^3.14.0",
    "c8": "^7.3.5",
    "documentation": "github:arlac77/documentation",
    "semantic-release": "^17.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/gitlab-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/gitlab-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/gitlab-repository-provider#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
