{
  "name": "gitlab-repository-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "npm@11.6.4+sha512.1118cab46a05a50aee6bff5b1b4fa1df18afff89d57465620a3518035026955db87c5bdf9d207b07b7487d99f2490d450cb774655ad63ec2cba7bf1d0ad25d45",
  "exports": {
    ".": "./src/gitlab-provider.mjs"
  },
  "description": "repository provider for gitlab hosted repos",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "0BSD",
  "scripts": {
    "test": "node --run test:ava",
    "test:ava": "ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src**/*.mjs",
    "lint": "node --run lint:docs",
    "lint:docs": "documentation lint ./src**/*.mjs"
  },
  "dependencies": {
    "content-entry": "^14.2.7",
    "matching-iterator": "^2.1.6",
    "one-time-execution-method": "^3.1.4",
    "repository-provider": "^36.1.0"
  },
  "devDependencies": {
    "ava": "^6.4.1",
    "c8": "^10.1.3",
    "documentation": "^14.0.3",
    "semantic-release": "^25.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/gitlab-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/gitlab-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/gitlab-repository-provider#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-javascript-component"
    ]
  }
}
